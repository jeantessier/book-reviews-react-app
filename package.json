{
  "name": "book-reviews",
  "version": "0.1.0",
  "private": true,
  "homepage": "/BookReviews",
  "devDependencies": {
    "react-scripts": "*"
  },
  "dependencies": {
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "react": "*",
    "react-dom": "*",
    "react-ga": "*",
    "react-markdown": "*",
    "react-script": "*",
    "rehype-raw": "*",
    "remark-gfm": "*",
    "web-vitals": "*"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(bail|ccount|character-entities|escape-string-regexp|hast-.*|hastscript|html-void-elements|is-plain-obj|markdown-table|mdast-.*|micromark.*|parse-entities|property-information|react-markdown|rehype-raw|remark-.*|.*-separated-tokens|trough|unified|unist-.*|vfile.*|web-namespaces|zwitch)/)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
